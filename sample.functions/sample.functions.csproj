  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Common.Construction.app\Architecture.foundation.Construction.app.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Update="host.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
