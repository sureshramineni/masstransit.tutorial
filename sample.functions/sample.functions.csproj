<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <AzureFunctionsVersion>v3</AzureFunctionsVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="MassTransit" Version="6.*" />
    <PackageReference Include="MassTransit.Extensions.DependencyInjection" Version="6.*" />
    <PackageReference Include="Microsoft.Azure.Functions.Extensions" Version="1.0.0" />
    <PackageReference Include="Microsoft.NET.Sdk.Functions" Version="3.0.7" />
    <PackageReference Include="MassTransit.Azure.ServiceBus.Core" Version="6.*" />
    <PackageReference Include="MassTransit.WebJobs.ServiceBus" Version="6.*" />
